{
  "name": "autobracket",
  "displayName": "AutoBracket",
  "description": "Automatically inserts missing closing brackets at the end of the line.",
  "version": "0.0.2",
  "publisher": "wuqzh8",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {"command": "autoBracket.CompleteParenthesis", "title": "complete parenthesis"},
      {"command": "autoBracket.CompleteCurlyBrace", "title": "complete curly brace"},
      {"command": "autoBracket.CompleteSquareBracket", "title": "complete square bracket"},
      {"command": "autoBracket.CompleteAuto", "title": "complete all brackets auto"}
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wuqzh8/autobracket"
  }
}
